{
  "name": "vscode-theme-elkon",
  "displayName": "Elkon theme",
  "version": "0.0.1",
  "description": "Elkon theme for VS Code",
  "keywords": [
    "theme",
    "vitesse",
    "light",
    "dark"
  ],
  "license": "MIT",
  "engines": {
    "vscode": "^1.66.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elkon028/vscode-theme-elkon"
  },
  "files": [
    "themes"
  ],
  "bugs": {
    "url": "https://github.com/elkon028/vscode-theme-elkon/issues"
  },
  "categories": [
    "Themes"
  ],
  "publisher": "elkon",
  "scripts": {
    "build": "esno src/index.ts",
    "dev": "nodemon --watch src -e ts --exec \"esno src/index.ts\"",
    "vscode:prepublish": "yarn build",
    "lint": "eslint .",
    "release": "yarn build && npx bumpp --commit --tag --push --all && vsce publish --no-dependencies"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.20.7",
    "@antfu/utils": "^0.5.1",
    "@types/color": "^3.0.3",
    "@types/node": "^17.0.27",
    "color": "^4.2.3",
    "eslint": "^8.14.0",
    "esno": "^0.14.1",
    "nodemon": "^2.0.15",
    "tsup": "^5.12.6",
    "typescript": "^4.6.3",
    "vsce": "^2.7.0"
  },
  "contributes": {
    "themes": [
      {
        "label": "Elkon dark",
        "uiTheme": "vs-dark",
        "path": "./themes/elkon-color-dark.json"
      }
    ]
  },
  "icon": "icon.png",
  "preview": true
}
